datasource db {
  provider = "mysql"
  url      = "mysql://root:password@localhost:3306/test_db?schema=public"
}

generator client {
  provider = "prisma-client-js"
  previewFeatures = ["metrics"]
}

model Todo {
  id        Int      @id @default(autoincrement())
  title     String
  completed Boolean  @default(false)
}
